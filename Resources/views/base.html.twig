<!DOCTYPE html>
<html lang="{{ app_locale }}">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    <title>{% block title %}Domainator 9K{% endblock %}</title>

    {% stylesheets filter='cssrewrite'
        'bundles/ctrlrad/vendor/bootstrap/dist/css/bootstrap.min.css'
        'bundles/ctrlrad/vendor/metisMenu/dist/metisMenu.min.css'
        'bundles/ctrlrad/vendor/startbootstrap-sb-admin-2/dist/css/sb-admin-2.css'
        'bundles/ctrlrad/vendor/font-awesome/css/font-awesome.min.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {% block stylesheets %}{% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

{% block body %}{% endblock %}

{% javascripts
    'bundles/ctrlrad/vendor/jquery/dist/jquery.min.js'
    'bundles/ctrlrad/vendor/bootstrap/dist/js/bootstrap.min.js'
    'bundles/ctrlrad/vendor/metisMenu/dist/metisMenu.min.js'
    'bundles/ctrlrad/vendor/metisMenu/dist/metisMenu.min.js'
    'bundles/ctrlrad/vendor/startbootstrap-sb-admin-2/dist/js/sb-admin-2.js'
%}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% block javascripts %}{% endblock %}

</body>

</html>
