{
    "name": "ctrl-f5/ctrl-rad-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CtrlRadBundle",
    "keywords": ["Symfony2", "RAD", "Bootstrap"],
    "homepage": "http://www.ctrl-f5.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicky De Maeyer",
            "email": "mythix.dev@gmail.com"
        }
    ],
    "bin": ["Resources/bin/set_cache_permissions"],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "3.0.*",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/user-bundle": "2.0.*",
        "twig/extensions": "~1.0",
        "knplabs/knp-menu-bundle": "~2.1",
        "ctrl-f5/ctrl-common": "2.0.*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "phpunit/phpunit": "5.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Ctrl\\RadBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
